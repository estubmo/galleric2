{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": ["paid", "unpaid"]
    },
    "total": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "product": {
      "via": "orders",
      "model": "product"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    }
  }
}
